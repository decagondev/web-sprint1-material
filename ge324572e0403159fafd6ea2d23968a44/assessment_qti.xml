          <respcondition continue="Yes">
            <conditionvar>
              <varequal respident="response1">2601</varequal>
            </conditionvar>
            <displayfeedback feedbacktype="Response" linkrefid="2601_fb"/>
          </respcondition>
          <respcondition continue="Yes">
