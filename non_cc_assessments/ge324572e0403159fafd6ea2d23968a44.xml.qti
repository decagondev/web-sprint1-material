            <respcondition continue="Yes">
              <conditionvar>
                <varequal respident="response1">8773</varequal>
              </conditionvar>
              <displayfeedback feedbacktype="Response" linkrefid="8773_fb"/>
            </respcondition>
            <respcondition continue="Yes">
              <conditionvar>
                <varequal respident="response1">3748</varequal>
